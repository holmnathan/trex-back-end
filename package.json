{
  "name": "trex-back-end",
  "version": "1.0.0",
  "type": "module",
  "description": "Back End API Server for Trex Travel Planning Web App",
  "main": "app.mjs",
  "scripts": {
    "start": "nodemon -r dotenv/config app.mjs",
    "seed": "node -r dotenv/config seed.mjs",
    "prettier": "prettier --write 'routes/*.mjs' 'models/*.mjs' 'controllers/*.mjs' 'config/*.mjs' 'config/strategies/*.mjs' '*.mjs'",
    "standard": "standard --fix",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanholm/trex-back-end.git"
  },
  "keywords": [
    "travel",
    "planner",
    "social",
    "expressjs",
    "mongodb",
    "vacation",
    "trip"
  ],
  "author": "Nathan Holm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nathanholm/trex-back-end/issues"
  },
  "homepage": "https://github.com/nathanholm/trex-back-end#readme",
  "dependencies": {
    "argon2": "^0.27.2",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "flow": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "mongoose-unique-validator": "^2.0.3",
    "ora": "^5.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-flow": "^7.13.13",
    "dotenv": "^8.2.0",
    "flow-bin": "^0.148.0"
  }
}
